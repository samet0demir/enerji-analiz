{
  "name": "energy-analytics-backend",
  "version": "1.0.1",
  "description": "Backend API for Turkey Energy Analytics Dashboard",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "railway:build": "npm install && npm run build",
    "backfill": "ts-node src/scripts/backfill-historical-data.ts",
    "backfill:ptf": "ts-node src/scripts/backfill-ptf-consumption.ts",
    "backfill:production": "ts-node src/scripts/backfill-production.ts",
    "backfill:weather": "ts-node src/scripts/backfill-weather.ts",
    "test:ptf": "ts-node src/scripts/test-ptf-consumption.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "better-sqlite3": "^12.4.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.7",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2"
  }
}
